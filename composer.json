{
    "name": "mattapril/laravel-responsable-reports",
    "description": "Framework for easily converting reports into HTTP responses",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "reports", "paginated", "content", "negotiation"],
    "authors": [
        {
            "name": "MattApril",
            "email": "Matthew.JApril@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^6|^7|^8",
        "illuminate/contracts": "^5.5|^6|^7|^8"
    },
    "require-dev": {
        "laravel/framework": "5.5.x",
        "phpunit/phpunit": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MattApril\\ResponsableReports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattApril\\ResponsableReports\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MattApril\\ResponsableReports\\ResponsableReportsProvider"
            ]
        }
    }
}
