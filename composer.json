{
    "name": "mattapril/laravel-responsable-reports",
    "description": "Library for easily converting reports into various HTTP responses",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "reports", "paginated", "content", "negotiation"],
    "authors": [
        {
            "name": "MattApril",
            "email": "Matthew.JApril@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^6|^7|^8|^9|^10|^11",
        "illuminate/contracts": "^5.5|^6|^7|^8|^9|^10|^11",
        "illuminate/routing": "^5.5|^6|^7|^8|^9|^10|^11"
    },
    "require-dev": {
        "laravel/framework": "^11.0",
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^9"

    },
    "autoload": {
        "psr-4": {
            "MattApril\\ResponsableReports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattApril\\ResponsableReports\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MattApril\\ResponsableReports\\ResponsableReportsProvider"
            ]
        }
    }
}
